<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.homolo.homolo.dao.AccountServiceDao">



    <select id="loadUserByUsername" resultType="java.util.HashMap">
        <![CDATA[
            SELECT userid, username FROM tq_accounts WHERE username=#{username}
        ]]>
    </select>



</mapper>